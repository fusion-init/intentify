{
    "compilerOptions": {
        "target": "es2021",
        "module": "commonjs",
        "lib": [
            "es2021"
        ],
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "composite": true,
        "moduleResolution": "node"
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}